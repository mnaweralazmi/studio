rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // Allow users to read, write, and delete their own files under a 'users/{userId}' path.
    // This rule ensures that users can only access files in their own designated folder.
    match /users/{userId}/{allPaths=**} {
      allow read, write: if request.auth != null && request.auth.uid == userId;
    }
  }
}
