rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    // Allow read and write access for all authenticated users.
    // This is a simple rule for development. For production, you should
    // implement more granular rules, e.g., allowing a user to write only
    // to their own folder.
    match /{allPaths=**} {
      allow read, write: if request.auth != null;
    }
  }
}
